<% provide(:title, "Gigs") %>
<div class="center jumbotron">
<h1>Gigs</h1>
<ul>
  <% @gigs.each do |gig| %>
    <li style="list-style-type: none;">
      Gig: <%= link_to gig.title, gig %>, 
      Location: <%= gig.location %>,
      Payment: <%= gig.payment %>,
      Deadline: <%= gig.deadline %>
    </li>
  <% end %>
</ul>
<%= link_to "New Gig", new_gig_path %>
</div>