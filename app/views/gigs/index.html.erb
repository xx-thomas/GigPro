<% provide(:title, "Gigs") %>
<div class="center jumbotron">
<h1><%=t(:gigs) %></h1>
<ul>
  <% @gigs.each do |gig| %>
    <li style="list-style-type: none;">
      <%=t(:gig)%>: <%= link_to gig.title, gig %>, 
      <%=t(:location)%>: <%= gig.location %>,
      <%=t(:payment)%>: <%= gig.payment %>,
      <%=t(:deadline)%>:  <%= gig.deadline %>
    </li>
  <% end %>
</ul>
<p></p>
<%= link_to t(:new_gig), new_gig_path, class: "btn btn-primary btn-lg" %>
</div>